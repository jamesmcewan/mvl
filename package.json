{
  "name": "mvl",
  "version": "7.7.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "eslint src/**/*.ts src/**/*.tsx *.ts *.mjs *.cjs",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx *.ts *.mjs *.cjs --fix",
    "format:write": "prettier --write .",
    "format:check": "prettier --check  .",
    "prepare": "is-ci || husky install",
    "lint-staged": "lint-staged",
    "test": "vitest",
    "update-dependencies": "npx npm-check-updates --upgrade && npm i"
  },
  "devDependencies": {
    "@astrojs/react": "^1.2.2",
    "@astrojs/tailwind": "^2.1.2",
    "@astrojs/vercel": "^2.3.4",
    "@tailwindcss/typography": "^0.5.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-react": "^2.2.0",
    "astro": "^1.6.10",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "happy-dom": "^7.7.0",
    "husky": "^8.0.2",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "prettier-plugin-astro": "^0.7.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.25.3"
  },
  "dependencies": {
    "ts-md5": "^1.3.1"
  },
  "lint-staged": {
    "*.md|*.html|*.json|*.astro|*.ts|*.tsx|*.mjs|*.cjs": [
      "prettier --write"
    ],
    "*.ts|.tsx|.cjs|*.mjs": [
      "eslint --fix"
    ]
  }
}
