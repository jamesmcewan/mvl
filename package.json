{
  "name": "mvl",
  "version": "8.1.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "eslint src/**/*.ts *.mjs *.cjs",
    "lint:fix": "eslint src/**/*.ts *.mjs *.cjs --fix",
    "format:write": "prettier --write .",
    "format:check": "prettier --check  .",
    "lint-staged": "lint-staged",
    "test": "vitest",
    "prepare": "is-ci || husky",
    "update-dependencies": "deno run -A npm:@astrojs/upgrade;deno run -A npm:npm-check-updates --upgrade && deno install"
  },
  "devDependencies": {
    "@astrojs/tailwind": "^5.1.1",
    "@deno/astro-adapter": "^0.1.3",
    "@tailwindcss/typography": "^0.5.15",
    "astro": "^4.15.12",
    "autoprefixer": "^10.4.20",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@astrojs/node": "^8.3.4",
    "solid-js": "^1.8.22",
    "ts-md5": "^1.3.1"
  },
  "lint-staged": {
    "*.md|*.html|*.json|*.astro|*.ts|*.tsx|*.mjs|*.cjs": [
      "deno lint"
    ],
    "*.ts|.tsx|.cjs|*.mjs": [
      "deno fmt"
    ]
  }
}
