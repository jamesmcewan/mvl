{
  "name": "mvl",
  "version": "7.2.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "ENABLE_VC_BUILD=1 astro build",
    "preview": "astro preview",
    "lint": "eslint src/**/*.ts src/**/*.tsx *.ts *.mjs *.cjs",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx *.ts *.mjs *.cjs --fix",
    "format:write": "prettier --write .",
    "format:check": "prettier --check  .",
    "prepare": "is-ci || husky install",
    "lint-staged": "lint-staged",
    "test": "vitest",
    "update-dependencies": "npx npm-check-updates --upgrade && npm i"
  },
  "devDependencies": {
    "@astrojs/react": "^0.1.2",
    "@astrojs/tailwind": "^0.2.1",
    "@astrojs/vercel": "^0.2.2",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/node": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vitejs/plugin-react": "^1.3.2",
    "astro": "^1.0.0-beta.29",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "happy-dom": "^3.2.2",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "prettier-plugin-astro": "^0.0.12",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.12.6"
  },
  "dependencies": {
    "ts-md5": "^1.2.11"
  },
  "lint-staged": {
    "*.md|*.html|*.json|*.astro|*.ts|*.tsx|*.mjs|*.cjs": [
      "prettier --write"
    ],
    "*.ts|.tsx|.cjs|*.mjs": [
      "eslint --fix"
    ]
  }
}
