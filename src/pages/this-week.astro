---
import getMyComics from '../data/getMyComics'
import Layout from '../layouts/Layout.astro'

const week = 'this'
let comics = []
let attributionText = ''
try {
  const response = await getMyComics(week)
  comics = response.comics
  attributionText = response.attributionText
} catch {}
---

{comics && <Layout {week} {comics} {attributionText} />}
